<div id="<%= dom_id expense %>">
  <p>
    <strong>Name:</strong>
    <%= expense.name %>
  </p>

  <p>
    <strong>Amount:</strong>
    <%= expense.amount %>
  </p>

  <p>
    <strong>User:</strong>
    <%= expense.user_id %>
  </p>

  <p>
    <strong>Categories:</strong>
    <ul>
    <% expense.categories.each do |category| %>
      <li><%=category.name%></li>
    <% end %>
    </ul>
  </p>

</div>
